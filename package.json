{
  "private": true,
  "scripts": {
    "start": "set PORT=9000 && umi dev",
    "build": "umi build",
    "postinstall": "umi generate tmp",
    "prettier": "prettier --write '**/*.{js,jsx,tsx,ts,less,md,json}'",
    "test": "umi-test",
    "test:coverage": "umi-test --coverage"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,jsx,less,md,json}": [
      "prettier --write"
    ],
    "*.ts?(x)": [
      "prettier --parser=typescript --write"
    ]
  },
  "dependencies": {
    "@ant-design/pro-layout": "^6.22.1",
    "highlight.js": "^11.2.0",
    "nprogress": "^0.2.0",
    "random-name": "^0.1.2",
    "react": "17.x",
    "react-adsense": "^0.1.0",
    "react-dom": "17.x",
    "rss-parser": "^3.12.0",
    "rss-to-json": "^2.0.1",
    "umi": "^3.5.13"
  },
  "devDependencies": {
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@umijs/preset-react": "1.x",
    "@umijs/test": "^3.5.13",
    "lint-staged": "^10.0.7",
    "prettier": "^2.2.0",
    "typescript": "^4.1.2",
    "yorkie": "^2.0.0"
  }
}
